{
    "name": "develpr/zencoder",
    "description": "A series of abstractions to simplify audio/video encoding with Zencoder",
    "keywords": ["zencoder", "encoder", "encoding"],
    "license": "BSD",
    "authors": [
        {
            "name": "Kevin Mitchell",
            "email": "kevin@develpr.com",
            "homepage": "http://develpr.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",        
        "illuminate/config": "1.*",
        "illuminate/container": "1.*",
        "illuminate/filesystem": "1.*",
        "illuminate/http": "1.*",
        "illuminate/routing": "1.*",
        "illuminate/support": "1.*"
    },    
    "autoload": {
        "psr-0": {
            "Zencoder": "src/"
        }
    },
    "minimum-stability": "stable"
}
